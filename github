/// first time from new folder
git init
git add .
git commit -m "first commit"
git branch -M main
git remote add origin https://github.com/Aakashdeveloper/userProfile.git
git push -u origin main

/// next all time from same folder
git add .
git commit -m "first commit"
git push

//clone

git clone https://github.com/Aakashdeveloper/userProfile
git pull

//////
git checkout -b branchname

git add .
git commit -m "code"
git push -u origin branchname


git status

git pull
merge code
